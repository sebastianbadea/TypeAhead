@{
    ViewBag.Title = "Simple remote";
}

<h2>Simple remote</h2>
<div class="col-md-6 col-md-offset-3 form-group">
    
    <input type="text" class="form-control typeahead" /> <br /><br />
    <input type="text" class="form-control" id="displayCtl" disabled="disabled" />
</div>



@Html.Hidden("urlGetDummyDict", Url.Action("GetDummyDict"))

@section scripts
{
    <script type="text/javascript">
        $(function () {
            type.simpleRemote($(".typeahead"), $("#urlGetDummyDict").val(), $("#displayCtl"));
        });
    </script>
}