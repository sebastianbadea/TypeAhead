@{
    ViewBag.Title = "Google map";
}

<h2>Google maps</h2>
<div class="col-md-6 col-md-offset-3">
    <div class="form-group">
        <input type="text" id="text" class="form-control typeahead" placeholder="search for a location" />
    </div>
    <div class="form-group">
        <label for="lat" class="control-label">Lat</label>
        <input type="text" class="form-control" id="lat" />
    </div>
    <div class="form-group">
        <label for="lng" class="control-label">Lng</label>
        <input type="text" class="form-control" id="lng" />
    </div>
    <div class="form-group">
        <label for="locationverify" class="control-label">Location</label>
        <input type="text" class="form-control" id="locationverify">
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        $(function () {
            type.googleMaps($(".typeahead"), $("#lat"), $("#lng"), $("#locationverify"));
        });
    </script>
}
